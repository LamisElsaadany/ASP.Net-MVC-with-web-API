        }
        function ToggleBusEditview() {
            $("#BusEditModal").modal('toggle');
        }
        function ToggleBusDeleteview() {
            $("#BusDeleteModal").modal('toggle');
        }

    </script>

    <script>

        $(function () {

            $.getJSON('../countries.json',
                function (data, status, myXhr) {

                    for (var prop in data) {
                        $('#Country').append('<option>' + prop + '</option>');
                    }


                    $('#Country').change(function () {
                        $("#lblCountryName").text($('#Country').val());
                        $('#City').empty();
                        var cntry = $('#Country').val();
                        for (var i = 0; i < data[cntry].length; i++) {

                            $('#City').append('<option>' + data[cntry][i] + '</option>');
                        }

                    });
                    $("#Country").select("Egypt");

                }
            );
            $('#City').change(function () {

                $("#lblCityName").text($('#City').val());
                $.getJSON('http://api.openweathermap.org/data/2.5/forecast?q=' + $('#City').val() + '&APPID=001b0f58045147663b1ea518d34d88b4&unit=metric&cnt=1', function (data) {
                    Temp = data.list[0].main;
                    var C = Math.floor(((parseFloat(Temp.temp) - 32) * (5 / 9)));
                    $('#Temp').text(C);
                    $("#Desc").text(data.list[0].weather[0].description);

                })

            });
            function EmailSent() {
                alert("Thank you for contacting us. We have received your message and will respond to you soon.");

            };
        });

    </script>
