        function ToggleCreateModal() {

            $("#CreateModal").modal("toggle");
        }

        function ToggleCreateDescModal() {

            $("#CreateDescModal").modal("toggle");
        }
        //News Edit Modal
        $(".EditNewsMainModal").modal('hide');

        function openNewsEditModal(c) {
            $(".EditNewsMainModal").modal('hide');
            $(".EditNewsMainModal:eq(" + c + ")").modal('show');
        }
        function ToggleEditModal() {
            $("#EditNewsModal").modal('hide');
        }

        //Description Edit Modal
        $(".EditDescMainModal").modal('hide');

        function openDescEditModal(c) {
            $(".EditDescMainModal").modal('hide');
            $(".EditDescMainModal:eq(" + c + ")").modal('show');
        }
        function ToggleEditDescModal() {
            $("#EditDescModal").modal('hide');
        }
        function ToggleBusAddationview() {
